{
  "name": "kucun",
  "version": "1.0.0",
  "description": "瓷砖行业库存管理工具",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "test:redis-ws": "tsx scripts/test-redis-websocket.ts",
    "cache:clear": "tsx scripts/clear-cache.ts",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "lint:staged": "lint-staged",
    "pre-commit": "lint-staged",
    "pre-push": "npm run type-check && npm run lint && npm run format:check && npm run build",
    "check-all": "npm run type-check && npm run lint && npm run format:check && npm run test:ci",
    "fix-eslint": "tsx scripts/fix-eslint-issues.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
