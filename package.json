    "ioredis": "^5.8.0",
    "lucide-react": "^0.400.0",
    "multer": "^1.4.5-lts.1",
    "next": "15.4.7",
    "next-auth": "^4.24.11",
    "qiniu": "^7.14.0",
    "react": "^18.3.1",
    "react-day-picker": "^9.10.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "sharp": "^0.33.0",
    "sonner": "^2.0.7",
